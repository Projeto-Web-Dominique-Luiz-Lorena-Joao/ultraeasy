<div id="<%= dom_id company %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= company.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ein:</strong>
    <%= company.ein %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= company.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Phone:</strong>
    <%= company.phone %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Address:</strong>
    <%= company.address %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Website:</strong>
    <%= company.website %>
  </div>
</div>
